
@{ 
    Layout = null;
} 

    <div class="modal fade" id="AddMaterialGrade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Add Material Grade</h4>
                </div>
                <div class="modal-body">
                    <form role="form">
                        <div class="form-group" hidden>
                            <label>Material Grade ID</label>
                            <input type="text" class="form-control" id="MaterialGradeId" placeholder="Material Grade ID">
                        </div>
                        <div class="form-group">
                            <label>Material Grade Name</label>
                            <input type="email" class="form-control" id="MaterialGradeName" placeholder="Material Grade Name">
                        </div>
                        <div class="form-group">
                            <label>Material Grade Rate</label>
                            <input type="text" class="form-control" id="MaterialGradeRate" placeholder="Material Grade Rate">
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary" id="btnAdd">Submit</button>
                </div>
            </div>

        </div>
    </div>

 